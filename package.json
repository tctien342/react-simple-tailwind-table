    "tailwindcss": "^3.1.8",
    "ts-jest": "^28.0.8",
    "tslib": "^2.3.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.5",
    "typescript-plugin-css-modules": "^3.4.0",
    "typescript-transform-paths": "^3.3.1",
    "vite": "^3.1.0"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'"
  },
