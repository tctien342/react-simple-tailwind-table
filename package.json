{
  "name": "react-simple-tailwind-table",
  "version": "0.1.0",
  "description": "An simple table with tailwindCSS for react",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "jsnext:main": "build/index.es.js",
  "style": "build/style.css",
  "scripts": {
    "build": "rollup -c",
    "dev": "start-storybook -p 6006",
    "test": "jest --coverage --maxWorkers=1",
    "lint": "eslint --cache --quiet --ext .js,.jsx,.ts,.tsx --fix .",
    "lint:staged": "lint-staged",
    "lint:style": "stylelint --fix 'src/**/*.{css,scss}'",
    "format": "prettier --write .",
    "format:staged": "pretty-quick --staged",
    "prepare": "husky install",
    "storybook": "yarn dev",
    "build-storybook": "build-storybook && mv ./storybook-static ./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tctien342/react-simple-tailwind-table.git"
  },
  "author": "tctien342",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tctien342/react-simple-tailwind-table/issues"
  },
  "homepage": "https://github.com/tctien342/react-simple-tailwind-table#readme",
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@faker-js/faker": "^7.5.0",
    "@lopatnov/rollup-plugin-uglify": "github:lopatnov/rollup-plugin-uglify",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.4.18",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^13.4.0",
    "@types/chroma-js": "^2.1.4",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^28.0.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "eslint": "^8.8.0",
    "eslint-config-stylelint": "^16.0.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-css-modules-transform": "^4.3.0",
    "jest-environment-jsdom": "^28.1.3",
    "jest-transform-css": "^4.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "postcss-csso": "^6.0.0",
    "postcss-import": "^15.0.0",
    "postcss-preset-env": "^7.3.1",
    "prettier": "^2.5.1",
    "prettier-stylelint": "^0.4.2",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts-ts": "^3.1.0",
    "react-test-renderer": "^18.2.0",
    "rollup": "^2.67.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
